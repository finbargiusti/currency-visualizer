{
  "name": "currency-visualizer",
  "version": "1.0.0",
  "repository": "https://github.com/finbargiusti/currency-visualizer.git",
  "author": "finbargiusti <finbargiusti@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "parcel index.html",
    "buildc": "emcc c/pack.c -O3 -o c/bin/cpack.js -s EXPORTED_FUNCTIONS=\"['_pack', '_malloc', '_free']\" -s EXPORTED_RUNTIME_METHODS=\"['ccall','cwrap', 'getValue', 'addFunction']\" -s MODULARIZE=1 -s EXPORT_ES6=1 -s ASYNCIFY -s ALLOW_TABLE_GROWTH=1",
    "build": "parcel build index.html"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.7.0",
    "lodash.debounce": "^4.0.8",
    "parcel-reporter-static-files-copy": "^1.4.0",
    "parcel-resolver-ignore": "^2.1.3",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10"
  },
  "dependencies": {
    "parcel": "^2.7.0"
  },
  "resolutions": {
    "lmdb": "2.5.3"
  },
  "parcelIgnore": [
    "js/pack.js",
    "js/pack.wasm"
  ]
}
